        "width": "100%"
      },
      "list": {
        "style": {
          "width": "80%",
          "//marginTop": "1rem"
        }
      },
      "effect": "coverflow",
      "centeredSlides": true,
      "slidesPerView": "auto",
      "//autoplay": false,
      "coverflowEffect": {
        "rotate": 0,
        "stretch": -55,
        "depth": 150,
        "modifier": 1,
        "slideShadows": false
